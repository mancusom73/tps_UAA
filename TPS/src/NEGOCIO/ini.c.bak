/***************************************************************************/
char OBTENER_NIVEL_LOG( char *valor )
/*
Función que carga el nivel de logs en la variable correspondiente
*/
/***************************************************************************/
{
  char nivel;
  //vemos si es si
  if(strcmp( valor, "si") == 0) //si
   nivel = '5' ;
  else
   if(strcmp( valor, "no") == 0) //no
      nivel = '0' ;
      else
         if( atoi(valor)>0 && atoi(valor)< 10 )
               nivel = atoi(valor) ;
          else
            nivel = '0' ;

 return nivel;
}

